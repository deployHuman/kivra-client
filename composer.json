{
    "name": "deployhuman/kivra-client",
    "license": "MIT",
    "description": "Client to access kivra API",
    "type": "library",
    "keywords": [
        "Kivra",
        "php",
        "sdk",
        "api"
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "DeployHuman\\kivra\\": "src/"
        }
    },
    "scripts": {
        "pint": [
            "@php ./vendor/bin/pint"
        ]
    }
}